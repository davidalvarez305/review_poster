<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in posts %}
    <url>
      <loc>https://{{ domain }}/{{ post.sub_category_slug }}/{{ post.slug }}</loc>
      <changefreq>monthly</changefreq>
      <priority>{{ post.priority }}</priority>
    </url>
  {% endfor %}
</urlset>